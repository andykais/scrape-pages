{
  "scrapers": {
    "archive": {
      "download": {
        "urlTemplate": "https://apod.nasa.gov/apod/archivepix.html"
      },
      "parse": {
        "selector": "body > b > a:nth-child(-n+10)",
        "attribute": "href"
      }
    },
    "image-page": {
      "download": "https://apod.nasa.gov/apod/{{value}}",
      "parse": {
        "selector": "body > center:nth-child(1) > p:nth-child(3) > a > img",
        "attribute": "src"
      }
    },
    "image": {
      "download": "https://apod.nasa.gov/apod/{{value}}"
    }
  },
  "run": {
    "scraper": "archive",
    "scrapeEach": {
      "scraper": "image-page",
      "scrapeEach": {
        "scraper": "image"
      }
    }
  }
}
