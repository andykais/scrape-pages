{
  "name": "scrape-pages",
  "version": "3.2.0",
  "description": "generalized scraping using a single config object for any site that can be scraped without a headless browser",
  "keywords": [
    "crawler",
    "scraper",
    "http",
    "emitter"
  ],
  "main": "index.js",
  "scripts": {
    "build": "run-s clean compile:runtime compile",
    "build:watch": "webpack --mode=development --watch",
    "compile": "webpack --mode=production",
    "compile:runtime:config": "ttsc --project src/settings/config/runtime/tsconfig.runtime.json",
    "compile:runtime:options": "ttsc --project src/settings/options/runtime/tsconfig.runtime.json",
    "compile:runtime:params": "ttsc --project src/settings/params/runtime/tsconfig.runtime.json",
    "compile:runtime": "run-s compile:runtime:config compile:runtime:options compile:runtime:params",
    "clean": "rimraf lib",
    "lint": "eslint --ignore-path .gitignore '**/*.ts'",
    "format": "git ls-files '*.ts' '*.json' | xargs prettier --write",
    "format:check": "git ls-files '*.ts' '*.json' | xargs prettier --list-different",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "typecheck:watch": "npm run typecheck -- --watch",
    "test:watch": "mochapack '{testing,src,examples}/**/*.test.ts' --watch",
    "test:unit": "mochapack src/**/*.unit.test.ts",
    "test:functional": "mochapack testing/**/*functional.test.ts",
    "test:examples": "mochapack examples/**/*.unit.test.ts",
    "test": "run-s typecheck test:unit test:functional test:examples lint format:check"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/andykais/scrape-pages.git"
  },
  "author": "Andrew Kaiser",
  "license": "MIT",
  "engines": {
    "node": "<=8.0"
  },
  "pre-push": [
    "test"
  ],
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@types/better-sqlite3": "^5.2.2",
    "@types/bunyan": "^1.8.5",
    "@types/chai": "^4.1.7",
    "@types/chai-jest-snapshot": "^1.3.4",
    "@types/cheerio": "^0.22.9",
    "@types/lodash": "^4.14.119",
    "@types/mocha": "^5.2.5",
    "@types/nock": "^9.3.0",
    "@types/node-fetch": "^2.1.2",
    "@types/sanitize-filename": "^1.1.28",
    "@types/source-map-support": "^0.4.1",
    "@types/verror": "^1.10.3",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "chai-exclude": "^2.0.1",
    "chai-jest-snapshot": "^2.0.0",
    "copy-webpack-plugin": "^4.5.2",
    "eslint": "^5.16.0",
    "eslint-plugin-no-only-tests": "^2.1.0",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "mocha-steps": "^1.3.0",
    "mochapack": "^1.1.1",
    "nock": "^10.0.3",
    "npm-run-all": "^4.1.5",
    "pre-push": "^0.1.1",
    "prettier": "^1.13.5",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.4",
    "terser-webpack-plugin": "^1.0.0",
    "ttypescript": "^1.5.6",
    "typescript": "^3.3.3333",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.0.6",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "better-sqlite3": "^5.2.1",
    "bunyan": "^1.8.12",
    "cheerio": "^1.0.0-rc.2",
    "handlebars": "^4.1.0",
    "node-fetch": "^2.1.2",
    "rxjs": "^6.4.0",
    "sanitize-filename": "^1.6.1",
    "source-map-support": "^0.5.6",
    "typescript-is": "^0.10.1",
    "verror": "^1.10.0"
  }
}
