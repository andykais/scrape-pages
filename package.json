{
  "name": "scrape-pages",
  "version": "3.0.1",
  "description": "generalized scraping using a single config object for any site that can be scraped without a headless browser",
  "main": "lib/main.js",
  "module": "src/index.js",
  "scripts": {
    "scratchwork": "node webpack.dev.js",
    "build": "webpack -p",
    "lint": "git ls-files '*.js' '*.json' | xargs prettier -l",
    "test": "jest",
    "test:unit": "jest unit",
    "test:integrations": "jest integration --detectOpenHandles",
    "prepublishOnly": "npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/andykais/scrape-pages.git"
  },
  "files": [
    "lib/*",
    "package-lock.json"
  ],
  "author": "Andrew Kaiser",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.3.2",
    "babel-plugin-flow-runtime": "^0.17.0",
    "babel-plugin-transform-async-generator-functions": "^6.24.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "flow-bin": "^0.75.0",
    "jest": "^23.1.0",
    "jest-raw-loader": "^1.0.1",
    "nodemon": "^1.17.5",
    "prettier": "^1.13.5",
    "raw-loader": "^0.5.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.6",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "better-sqlite3": "^4.1.4",
    "cheerio": "^1.0.0-rc.2",
    "flow-runtime": "^0.17.0",
    "node-fetch": "^2.1.2",
    "rxjs": "^6.2.0",
    "sanitize-filename": "^1.6.1",
    "source-map-support": "^0.5.6",
    "string-template": "^1.0.0"
  },
  "jest": {
    "setupFiles": [
      "./tests/setup.js"
    ],
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "roots": [
      "<rootDir>/examples",
      "<rootDir>/src"
    ],
    "transform": {
      "\\.sql$": "jest-raw-loader",
      "\\.js$": "babel-jest"
    },
    "globals": {
      "NODE_ENV": "test"
    }
  }
}
