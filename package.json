{
  "name": "scrape-pages",
  "version": "3.4.1",
  "description": "generalized scraping using a single config object for any site that can be scraped without a headless browser",
  "keywords": [
    "crawler",
    "emitter",
    "http",
    "scraper"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/andykais/scrape-pages.git"
  },
  "license": "MIT",
  "author": "Andrew Kaiser",
  "main": "./index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "build": "run-s clean compile compile:declarations",
    "build:watch": "webpack --mode=development --watch",
    "clean": "shx rm -rf lib node_modules/.cache/tsconfig.buildinfo",
    "compile": "webpack --mode=production",
    "compile:declarations": "tsc --project ./tsconfig/tsconfig.declarations.json",
    "format": "prettier --write --ignore-path=.gitignore '**/*.{json,ts,js}'",
    "format:check": "prettier --check --ignore-path=.gitignore '**/*.{json,ts,js}'",
    "lint": "eslint --ignore-path .gitignore '**/*.ts'",
    "test": "run-s typecheck test:unit test:functional test:examples lint format:check",
    "test:coverage": "nyc mochapack --webpack-env=coverage '{testing,src,examples}/**/*.ts'",
    "test:examples": "mochapack --interactive=false examples/**/*.unit.test.ts",
    "test:functional": "mochapack --interactive=false testing/**/*functional.test.ts",
    "test:unit": "mochapack --interactive=false src/**/*.unit.test.ts",
    "test:watch": "mochapack '{testing,src,examples}/**/*.ts' --watch",
    "typecheck": "tsc --noEmit --project tsconfig/tsconfig.json",
    "typecheck:watch": "npm run typecheck -- --watch"
  },
  "dependencies": {
    "better-sqlite3": "^5.4.3",
    "bunyan": "^1.8.12",
    "cheerio": "^1.0.0-rc.3",
    "handlebars": "^4.4.3",
    "jsonata": "^1.7.0",
    "node-fetch": "^2.5.0",
    "rxjs": "^6.5.2",
    "sanitize-filename": "^1.6.1",
    "typescript-is": "^0.12.6",
    "verror": "^1.10.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^5.4.0",
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.2.3",
    "@types/cheerio": "^0.22.13",
    "@types/mocha": "^5.2.6",
    "@types/mocha-steps": "^1.1.1",
    "@types/node-fetch": "^2.5.2",
    "@types/readline-sync": "^1.4.3",
    "@types/verror": "^1.10.3",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^5.0.3",
    "coveralls": "^3.0.3",
    "eslint": "^6.5.1",
    "eslint-plugin-no-only-tests": "^2.3.0",
    "glob": "^7.1.4",
    "istanbul-instrumenter-loader": "^3.0.1",
    "mocha": "^6.2.1",
    "mocha-steps": "^1.3.0",
    "mochapack": "^1.1.5",
    "nock": "^11.4.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "pre-push": "^0.1.1",
    "prettier": "^1.17.1",
    "raw-loader": "^3.1.0",
    "readline-sync": "^1.4.10",
    "shx": "^0.3.2",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-node-externals": "^1.7.2"
  },
  "engines": {
    "node": ">=8.0"
  },
  "pre-push": [
    "test"
  ]
}
