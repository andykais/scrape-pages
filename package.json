{
  "name": "scrape-pages",
  "version": "3.3.0",
  "description": "generalized scraping using a single config object for any site that can be scraped without a headless browser",
  "keywords": [
    "crawler",
    "emitter",
    "http",
    "scraper"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/andykais/scrape-pages.git"
  },
  "license": "MIT",
  "author": "Andrew Kaiser",
  "main": "./index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "build": "run-s clean compile compile:declarations",
    "build:watch": "webpack --mode=development --watch",
    "clean": "shx rm -rf lib node_modules/.cache/tsconfig.buildinfo",
    "compile": "webpack --mode=production",
    "compile:declarations": "tsc --project ./tsconfig/tsconfig.declarations.json",
    "format": "prettier --write --ignore-path=.gitignore '**/*.{json,ts,js}'",
    "format:check": "prettier --check --ignore-path=.gitignore '**/*.{json,ts,js}'",
    "lint": "eslint --ignore-path .gitignore '**/*.ts'",
    "test": "run-s typecheck test:unit test:functional test:examples lint format:check",
    "test:coverage": "nyc mochapack --webpack-env=coverage '{testing,src,examples}/**/*.ts'",
    "test:examples": "mochapack examples/**/*.unit.test.ts",
    "test:functional": "mochapack testing/**/*functional.test.ts",
    "test:unit": "mochapack src/**/*.unit.test.ts",
    "test:watch": "mochapack '{testing,src,examples}/**/*.ts' --watch",
    "typecheck": "tsc --noEmit --project tsconfig/tsconfig.json",
    "typecheck:watch": "npm run typecheck -- --watch"
  },
  "dependencies": {
    "better-sqlite3": "^5.4.0",
    "bunyan": "^1.8.12",
    "cheerio": "^1.0.0-rc.3",
    "handlebars": "^4.1.1",
    "jsonpath-plus": "^0.18.1",
    "node-fetch": "^2.5.0",
    "rxjs": "^6.5.2",
    "sanitize-filename": "^1.6.1",
    "typescript-is": "^0.10.6",
    "verror": "^1.10.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^5.4.0",
    "@types/bunyan": "^1.8.6",
    "@types/chai": "^4.1.7",
    "@types/chai-jest-snapshot": "^1.3.4",
    "@types/cheerio": "^0.22.11",
    "@types/lodash": "^4.14.129",
    "@types/mocha": "^5.2.6",
    "@types/mocha-steps": "^1.1.1",
    "@types/nock": "^10.0.3",
    "@types/node-fetch": "^2.3.3",
    "@types/sanitize-filename": "^1.1.28",
    "@types/verror": "^1.10.3",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "awesome-typescript-loader": "^5.2.1",
    "chai": "^4.2.0",
    "chai-exclude": "^2.0.1",
    "chai-jest-snapshot": "^2.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-no-only-tests": "^2.3.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "lodash": "^4.17.11",
    "mocha": "^6.1.4",
    "mocha-steps": "^1.3.0",
    "mochapack": "^1.1.1",
    "nock": "^10.0.6",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "pre-push": "^0.1.1",
    "prettier": "^1.17.1",
    "raw-loader": "^2.0.0",
    "shx": "^0.3.2",
    "ttypescript": "^1.5.6",
    "typescript": "3.5.3",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-node-externals": "^1.7.2"
  },
  "engines": {
    "node": ">=8.0"
  },
  "pre-push": [
    "test"
  ]
}
