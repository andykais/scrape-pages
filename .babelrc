{
  presets: [
    [
      'env',
      {
        targets: {
          node: 8
        }
      }
    ],
    'stage-2'
  ],
  plugins: [
    'transform-decorators-legacy',
    'transform-flow-strip-types',
    [
      'flow-runtime',
      {
        assert: false,
        annotate: false,
        warn: false
        // optInOnly: true
      }
    ],
    'add-module-exports'
  ],
  env: {
    test: {
      presets: [['env'], 'stage-2', 'flow']
    }
  }
}
