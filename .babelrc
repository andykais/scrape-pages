{
  presets: [
    [
      'env',
      {
        target: {
          node: 'current'
        }
      }
    ]
  ],
  plugins: [
    'transform-decorators-legacy',
    'transform-class-properties',
    'transform-object-rest-spread',
    'transform-async-generator-functions',
    'transform-async-to-generator',
    'transform-flow-strip-types',
    [
      'transform-runtime',
      {
        polyfill: false,
        regenerator: true
      }
    ],
    [
      'flow-runtime',
      {
        assert: false,
        annotate: false,
        warn: false
        // optInOnly: true
      }
    ],
    'add-module-exports'
  ],
  env: {
    test: {
      presets: [['env'], 'stage-2', 'flow']
    }
  }
}
