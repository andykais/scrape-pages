{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": "8.0"
        }
      }
    ],
    "stage-2"
  ],
  "plugins": [
    "transform-flow-strip-types",
    [
      "flow-runtime",
      {
        "assert": false,
        "annotate": false
      }
    ],
    "add-module-exports"
  ]
}
